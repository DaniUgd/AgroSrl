<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="Clases.Proyecto" table="Proyecto">
  <id name="idProyecto" type="int">
            <column name="IDPROYECTO" />
            <generator class="native" />
    </id>
    <property name="descripcion" type="java.lang.String">
             <column name="DESCRIPCION"/>
    </property>
    <list name="lotes" cascade="none">
        <key column="FKPROYECTO"/>
        <index column=""/>
        <one-to-many class="Clases.Lotes"/>
    </list>
    <many-to-one name="estado" cascade="none" class="Clases.EstadoProyecto">
        <column name="FKESTADOP"/>
    </many-to-one>
    <many-to-one name="tproyecto" cascade="none" class="Clases.TipoProyecto">
        <column name="FKTIPOPROYECTO"/>
    </many-to-one>
    

</class>


</hibernate-mapping>
