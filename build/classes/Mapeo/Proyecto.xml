<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="Clases.Proyecto" table="Proyecto">
  <id name="idProyecto" type="int">
            <column name="IDPROYECTO" />
            <generator class="native" />
    </id>
    <property name="descipcion" type="java.lang.String">
             <column name="DESCRIPCION"/>
    </property>
    <list name="laboreo" cascade="none">
        <key column="IDLABOREO"/>
        <index column="ORDEN"/>
        <one-to-many class="Clases.TipoLaboreo"/>
    </list>
  </class>


</hibernate-mapping>
